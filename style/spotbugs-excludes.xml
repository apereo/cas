<FindBugsFilter>
    <Match>
        <Class name="~.*\.*Test.*"/>
        <Not>
            <Bug code="IJU"/>
        </Not>
    </Match>
    <Match>
        <Or>
            <Package name="~.*generated.*" />
        </Or>
    </Match>
    <Match>
        <Bug pattern="RR_NOT_CHECKED, EI_EXPOSE_REP, EI_EXPOSE_REP2, SE_TRANSIENT_FIELD_NOT_RESTORED" />
    </Match>
</FindBugsFilter>
