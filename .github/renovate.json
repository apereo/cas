  "gradle": {
    "enabled": true
  },
  "ignoreDeps": [
    "com.github.coova:jradius",
    "com.mebigfatguy.fb-contrib:fb-contrib"
  ],
  "packageRules": [
    {
      "groupName": "Groovy",
      "packagePatterns": [
        "^org.codehaus.groovy:"
      ]
    },
    {
      "groupName": "Groovy",
      "packagePatterns": [
        "^de.codecentric:spring-boot-admin-starter"
      ]
    },
    {
      "groupName": "SpringBoot",
      "packagePatterns": [
        "^org.springframework.boot:"
      ]
    },
    {
      "groupName": "Spring",
      "packagePatterns": [
        "^org.springframework:"
      ]
    },
    {
      "groupName": "SpringSession",
      "packagePatterns": [
        "^org.springframework.session:"
      ]
    },
    {
      "groupName": "Ribbon",
      "packagePatterns": [
