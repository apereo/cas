<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>${basedir}/../project.xml</extend>
  <pomVersion>3</pomVersion>
  <artifactId>cas-server-ldap</artifactId>
  <name>JA-SIG Central Authentication Service - LDAP AuthenticationHandlers</name>
  <siteDirectory></siteDirectory>
  <repository>
    <connection>scm:cvs:pserver:anonymous@developer.ja-sig.org:/home/cvs/jasig:cas</connection>
    <developerConnection>scm:cvs:pserver:${maven.username}@developer.ja-sig.org:/home/cvs/jasig:cas</developerConnection>
    <url>http://developer.ja-sig.org/source/viewrep/jasig/cas3/adaptors/ldap</url>
  </repository>
  
	<dependencies>
  		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-ldap</artifactId>
			<version>1.1.2</version>
			<type>jar</type>
			<url>http://www.springframework.org/ldap</url>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>	
		
  		<dependency>
			<groupId>com.sun.jndi.ldap</groupId>
			<artifactId>ldapbp</artifactId>
			<version>1.0</version>
			<type>jar</type>
			<url>http://www.sun.com</url>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>	
		
  		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.1</version>
			<type>jar</type>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>	
  	</dependencies>
</project>

