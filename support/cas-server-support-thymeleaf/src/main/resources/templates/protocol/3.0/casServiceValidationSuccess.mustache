        <cas:user>{{principal.id}}</cas:user>{{#pgtIou}}
        <cas:proxyGrantingTicket>{{pgtIou}}</cas:proxyGrantingTicket>{{/pgtIou}}
        {{#proxies.0}}
            <cas:proxies>
                {{#proxies}}
                <cas:proxy>{{{.}}}</cas:proxy>
                {{/proxies}}
            </cas:proxies>
        {{/proxies.0}}
        {{#formattedAttributes.0}}
