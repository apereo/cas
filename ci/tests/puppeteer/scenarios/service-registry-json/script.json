{
  "dependencies": "json-service-registry",
  "conditions": {
      "docker": "true"
  },
  "properties": [
    "--cas.server.name=https://localhost:8443",
    "--cas.server.prefix=${cas.server.name}/cas",

    "--cas.service-registry.core.init-from-json=false",
